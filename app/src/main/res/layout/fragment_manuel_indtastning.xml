<?xml version="1.0" encoding="utf-8"?>

<layout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools">

    <data>
    <variable
        name="dataViewModel"
        type="com.example.openlog.viewModel.DataViewModel" />
    </data>

<androidx.constraintlayout.widget.ConstraintLayout

    android:id="@+id/frameLayout4"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".view.ManuelIndtastning">


    <!-- TODO: Update blank fragment layout -->

    <ImageButton
            android:id="@+id/blodsukkerButton"
            android:layout_width="101dp"
            android:layout_height="94dp"
            android:backgroundTint="@color/white"
            android:scaleType="centerInside"
            android:src="@drawable/blodsukker"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.339"
            app:layout_constraintHorizontal_bias="0.493" />

    <ImageButton
            android:id="@+id/kulhydraterButton"
            android:layout_width="94dp"
            android:layout_height="94dp"
            android:backgroundTint="@color/white"
            android:scaleType="centerInside"
            android:src="@drawable/kulhydrater"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.807"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.339" />

    <ImageButton
            android:id="@+id/insulinButton"
            android:layout_width="95dp"
            android:layout_height="94dp"
            android:backgroundTint="@color/white"
            android:scaleType="centerInside"
            android:src="@drawable/insulin"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.186"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.339" />

    <ImageButton
            android:id="@+id/imageButton15"
            android:layout_width="152dp"
            android:layout_height="151dp"
            android:backgroundTint="@color/white"
            android:scaleType="centerInside"
            android:src="@drawable/mikrofonny"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="1.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.998" />

    <EditText
        android:id="@+id/textInput"
        android:layout_width="281dp"
        android:layout_height="62dp"
        android:ems="10"
        android:visibility="invisible"
        android:inputType="number"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.499" />

    <Button
        android:id="@+id/logButton"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:backgroundTint="#069C81"
        android:text="@string/logdata"
        android:visibility="invisible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.51"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.603" />

    <ImageButton
            android:layout_width="110dp"
            android:layout_height="110dp"
            app:srcCompat="@drawable/blodsukkerm"
            android:id="@+id/blodsukkerhighlight"
            android:scaleType="centerInside"
            android:backgroundTint="@color/white"
            android:visibility="invisible"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.498"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.328" />

    <ImageButton
            android:layout_width="110dp"
            android:layout_height="110dp"
            app:srcCompat="@drawable/insulinm"
            android:id="@+id/Insulinhighlight"
            android:scaleType="centerInside"
            android:backgroundTint="@color/white"
            android:visibility="invisible"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.142"
            app:layout_constraintVertical_bias="0.328" />

    <ImageButton
            android:layout_width="110dp"
            android:layout_height="110dp"
            app:srcCompat="@drawable/kulhydratm"
            android:id="@+id/kulhydraterhighlight"
            android:backgroundTint="@color/white"
            android:scaleType="centerInside"
            android:visibility="invisible"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintVertical_bias="0.328"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintHorizontal_bias="0.863" />

</androidx.constraintlayout.widget.ConstraintLayout>
</layout>